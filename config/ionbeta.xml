<config>

    <isotope name="82nb">
        <timeScale> 1e6 </timeScale>
        <timeThreshold> 500 </timeThreshold>
    </isotope>

    <isotope name="84nb">
        <timeScale> 1e9 </timeScale>
        <timeThreshold> 50 </timeThreshold>
    </isotope>
    
    <isotope name="81zr">
        <timeScale> 1e9 </timeScale>
        <timeThreshold> 30 </timeThreshold>
    </isotope>

    <window name = "decayGamma">
        <start> -18200 </start>
        <end> -12900 </end>
    </window>

    <window name = "gammaGamma">
        <start> -500 </start>
        <end> 500 </end>
    </window>

    <dssd> 1 </dssd>                            
    <plotBetaGamma> true </plotBetaGamma>       

    <implantDeadtime> 350e3 </implantDeadtime>
    <totalDeadtimePositionWindow> 5. </totalDeadtimePositionWindow>

    <vetoInterruptedImplant> true </vetoInterruptedImplant>
    <onlyOffspillDecay> false </onlyOffspillDecay>
    <allowAjacentClusters> true </allowAjacentClusters>

    <betaGammaCandidateCut> 5 </betaGammaCandidateCut>
    <correlationPositionWindow> 1 </correlationPositionWindow> <!-- abs(x - x_other) <= correlationPositionWindow --> 

    <brokenAidaStripsDecayX> 63 64 66 67 128 130 189 191 192 194 </brokenAidaStripsDecayX>
    <brokenAidaStripsDecayY></brokenAidaStripsDecayY>
    <brokenAidaStripsImplantX></brokenAidaStripsImplantX>
    <brokenAidaStripsImplantY></brokenAidaStripsImplantY>

    <histograms>
        <!-- Add histogram constants here -->
    </histograms>

    <eventConditions>
        <!-- Add histogram constants here -->
    </eventConditions>

</config>
